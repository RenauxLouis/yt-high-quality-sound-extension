{
  "manifest_version": 3,
  "name": "TRUE ASMR PROTOTYPE",
  "version": "1.2.9",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["bfy_content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icons/iconon-38.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}