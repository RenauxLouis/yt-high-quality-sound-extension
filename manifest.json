{
  "manifest_version": 2,
  "name": "Music play",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"],
      "js": [
        "bfy_content.js"
      ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://www.youtube.com/*"
  ]
}